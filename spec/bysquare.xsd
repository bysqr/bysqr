<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.bysquare.com/bysquare" xmlns:bsqr="http://www.bysquare.com/bysquare" elementFormDefault="qualified">
	<!-- Atribúty rozširujúce funkčnost by square schémy. -->
	<attribute name="priority" type="integer">
		<annotation>
			<documentation>Priorita pre skracovanie ak celková dĺžka výstupného reťazca je väčšia ako odporúčaná maximálna dĺžka definovaná pre konkrétny by square typ. Polia s číselne nižšou prioritou sú skracované najskôr.</documentation>
		</annotation>
	</attribute>
	<attribute name="maxLength" type="integer">
		<annotation>
			<documentation>Odporúčaná maximálna dĺžka poľa vo výstupnom reťazci (sekvencii). Používa sa pri skracovaní, kedy celková dĺžka výstupného reťazca (sekvencie) je väčšia ako odporúčaná maximálna dĺžka definovaná pre konkrétny typ dokumentu.</documentation>
		</annotation>
	</attribute>
	<attribute name="computed" type="boolean" default="false">
		<annotation>
			<documentation>Ak je true, vybrané pole nie je súčasťou výstupného reťazca (sekvencie). Pre elementy and atribúty.</documentation>
		</annotation>
	</attribute>
	<attribute name="order" type="integer" default="0">
		<annotation>
			<documentation>Pozícia poľa vo výstupnom reťazci (sekvencii). Rekurzívne pre komplexné typy. Pre elementy, atribúty, skupiny, skupiny atribútov a rozšírenia.</documentation>
		</annotation>
	</attribute>
	<attribute name="bysquareType" type="integer">
		<annotation>
			<documentation>Kódové číslo by square typu. Len pre koreňové elementy.</documentation>
		</annotation>
	</attribute>
	<attribute name="version" type="integer">
		<annotation>
			<documentation>Verzia by square typu. Len pre koreňové elementy.</documentation>
		</annotation>
	</attribute>
	<attribute name="documentType" type="integer">
		<annotation>
			<documentation>Kódové číslo typu dokumentu. Pre elementy.</documentation>
		</annotation>
	</attribute>
	<!-- BYSQUARE TYPES -->
	<complexType name="BySquareDocument" abstract="true" >
		<annotation>
			<documentation>Abstraktný rodičovský typ pre všetky by square XML dokumenty.</documentation>
		</annotation>
	</complexType>
	<element name="Pay" type="bsqr:PayBase" bsqr:bysquareType="0" bsqr:version="0">
		<annotation>
			<documentation>Dokument platobného QR kódu podľa štandardu by square.</documentation>
		</annotation>
	</element>
	<element name="Invoice" type="bsqr:InvoiceBase" bsqr:bysquareType="1" bsqr:version="0">
		<annotation>
			<documentation>Dokument QR kódu faktúry podľa štandardu by square.</documentation>
		</annotation>
	</element>
	<element name="InvoiceItems" type="bsqr:InvoiceItemsBase" bsqr:bysquareType="2"
		bsqr:version="0">
		<annotation>
			<documentation>Dokument QR kódu položiek faktúry podľa štandardu by square.</documentation>
		</annotation>
	</element>
	<!-- DOCUMENT TYPES -->
	<!-- PAY -->
	<complexType name="Pay" bsqr:documentType="0">
        <annotation>
        	<documentation>Hlavný typ pre všetky by square XML platobné QR kódy.</documentation>
        </annotation>
        <complexContent>
			<extension base="bsqr:PayBase" bsqr:order="1" />
		</complexContent>
	</complexType>
	<!-- INVOICE ITEMS -->
	<complexType name="InvoiceItems" bsqr:documentType="0">
		<annotation>
			<documentation>Hlavný typ pre všetky by square XML zoznamy položiek faktúry.</documentation>
		</annotation>
		<complexContent>
			<extension base="bsqr:InvoiceItemsBase"></extension>
		</complexContent>
	</complexType>
	<!-- INVOICE -->
	<complexType name="Invoice" bsqr:documentType="0">
        <annotation>
        	<documentation>Dokument QR kódu faktúry podľa štandardu by square, typ: faktúra.</documentation>
        </annotation>
        <complexContent>
			<extension base="bsqr:InvoiceBase" />
		</complexContent>
	</complexType>
	<complexType name="ProformaInvoice" bsqr:documentType="1">
        <annotation>
        	<documentation>Dokument QR kódu faktúry podľa štandardu by square, typ: proforma faktúra.</documentation>
        </annotation>
        <complexContent>
			<extension base="bsqr:InvoiceBase" />
		</complexContent>
	</complexType>
	<complexType name="CreditNote" bsqr:documentType="2">
        <annotation>
        	<documentation>Dokument QR kódu faktúry podľa štandardu by square, typ: dobropis.</documentation>
        </annotation>
        <complexContent>
			<extension base="bsqr:InvoiceBase" />
		</complexContent>
	</complexType>
	<complexType name="DebitNote" bsqr:documentType="3">
        <annotation>
        	<documentation>Dokument QR kódu faktúry podľa štandardu by square, typ: ťarchopis.</documentation>
        </annotation>
        <complexContent>
			<extension base="bsqr:InvoiceBase" />
		</complexContent>
	</complexType>
	<complexType name="AdvanceInvoice" bsqr:documentType="4">
        <annotation>
        	<documentation>Dokument QR kódu faktúry podľa štandardu by square, typ: zálohová faktúra.</documentation>
        </annotation>
        <complexContent>
			<extension base="bsqr:InvoiceBase" />
		</complexContent>
	</complexType>
	<!-- COMMON TYPES -->
	<simpleType name="CurrencyCode">
		<annotation>
			<documentation>Mena v ISO 4217 formáte (3 písmená). Príklad: "EUR".</documentation>
		</annotation>
		<restriction base="string">
			<pattern value="[A-Z]{3}" />
		</restriction>
	</simpleType>
	<simpleType name="CountryCode">
		<annotation>
			<documentation>Krajina v ISO 3166 formáte (3 písmená skratka). Príklad: SVK</documentation>
		</annotation>
		<restriction base="string">
			<pattern value="[A-Z]{3}" />
		</restriction>
	</simpleType>
	<simpleType name="Percentage">
		<annotation>
			<documentation>Percentuálna hodnota vyjadrená v normovanom tvare.

Príklad:
0% sa uvádza ako "0".
1% sa uvádza ako "0.01".
10% sa uvádza ako "0.1".
50% sa uvádza ako "0.5".
100% sa uvádza ako "1".</documentation>
		</annotation>
		<restriction base="decimal">
			<minInclusive value="0" />
			<maxInclusive value="1" />
		</restriction>
	</simpleType>
	<!-- PAY BY SQUARE -->
	<complexType name="PayBase" abstract="true">
        <annotation>
        	<documentation>Abstraktný rodičovský typ pre všetky by square XML platobné QR kódy.</documentation>
        </annotation>
        <complexContent>
			<extension base="bsqr:BySquareDocument">
				<sequence>
					<element name="InvoiceID" type="string" minOccurs="0" bsqr:maxLength="10"
						bsqr:priority="2" bsqr:order="1">
						<annotation>
							<documentation>Číslo faktúry v prípade, že údaje sú súčasťou faktúry, alebo identifikátor pre intérne potreby vystavovateľa.</documentation>
						</annotation>
					</element>
					<element name="Payments" type="bsqr:Payments" bsqr:order="2">
						<annotation>
							<documentation>Zoznam jednej alebo viacerých platieb v prípade hromadného príkazu. Hlavná (preferovaná) platba sa uvádza ako prvá.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>
	<complexType name="Payments">
        <annotation>
        	<documentation>Zoznam platieb.</documentation>
        </annotation>
        <sequence>
			<element name="Payment" type="bsqr:Payment" maxOccurs="unbounded" bsqr:order="1">
				<annotation>
					<documentation>Údaje pre jeden platobný príkaz.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="Payment">
        <annotation>
        	<documentation>Údaje pre platobný príkaz.</documentation>
        </annotation>
        <sequence>
			<element name="PaymentOptions" type="bsqr:PaymentOptions" bsqr:maxLength="1"
				bsqr:priority="999" bsqr:order="1">
				<annotation>
					<documentation>Možnosti platby sa dajú kombinovať. Oddeľujú sa medzerou a treba uviesť vždy aspoň jednu z možností.
paymentorder - platobný príkaz
standingorder - trvalý príkaz, údaje sa vyplnia do StandingOrderExt
directdebit - inkaso, údaje sa vyplnia do DirectDebitExt</documentation>
				</annotation>
			</element>
			<element name="Amount" type="decimal" minOccurs="0" bsqr:maxLength="15"
				bsqr:priority="999" bsqr:order="2">
				<annotation>
					<documentation>Čiastka platby. Povolené sú len kladné hodnoty. Desatinná čast je oddelená bodkou. 
Môže ostať nevyplnené, napríklad pre dobrovoľný príspevok (donations).

Príklad:
Tisíc sa uvádza ako "1000".
Jedna celá deväťdesiatdeväť sa uvádza ako "1.99".
Desať celých peťdesiat sa uvádza ako "10.5".
Nula celá nula osem sa uvádza ako "0.08".</documentation>
				</annotation>
			</element>
			<element name="CurrencyCode" type="bsqr:CurrencyCode" bsqr:maxLength="3"
				bsqr:priority="999" bsqr:order="3">
				<annotation>
					<documentation>Mena platby v ISO 4217 formáte (3 písmená skratka). Príklad: "EUR".</documentation>
				</annotation>
			</element>
			<element name="PaymentDueDate" type="date" minOccurs="0" bsqr:maxLength="8"
				bsqr:priority="999" bsqr:order="4">
				<annotation>
					<documentation>Dátum splatnosti vo formáte ISO 8601 "RRRR-MM-DD".  Nepovinný údaj.
V prípade trvalého príkazu označuje dátum prvej platby.</documentation>
				</annotation>
			</element>
			<choice minOccurs="0">
				<sequence>
					<element name="VariableSymbol" type="bsqr:VariableSymbol" minOccurs="0"
						bsqr:maxLength="10" bsqr:priority="7" bsqr:order="5">
						<annotation>
							<documentation>Variabilný symbol je maximálne 10 miestne číslo. Nepovinný údaj.</documentation>
						</annotation>
					</element>
					<element name="ConstantSymbol" type="bsqr:ConstantSymbol" minOccurs="0"
						bsqr:maxLength="4" bsqr:priority="5" bsqr:order="6">
						<annotation>
							<documentation>Konštantný symbol je 4 miestne identifikačné číslo. Nepovinný údaj.</documentation>
						</annotation>
					</element>
					<element name="SpecificSymbol" type="bsqr:SpecificSymbol" minOccurs="0"
						bsqr:maxLength="10" bsqr:priority="6" bsqr:order="7">
						<annotation>
							<documentation>Špecifický symbol je maximálne 10 miestne číslo. Nepovinný údaj.</documentation>
						</annotation>
					</element>
				</sequence>
				<element name="OriginatorsReferenceInformation" type="string" minOccurs="0"
					bsqr:maxLength="35" bsqr:priority="12" bsqr:order="8">
					<annotation>
						<documentation>Referenčná informácia prijímateľa podľa SEPA.</documentation>
					</annotation>
				</element>
			</choice>
			<element name="PaymentNote" type="string" minOccurs="0" bsqr:maxLength="140"
				bsqr:priority="1" bsqr:order="9">
				<annotation>
					<documentation>Správa pre prijímateľa. Údaje o platbe, na základe ktorých príjemca bude môcť platbu identifikovať.
Odporúča sa maximálne 140 Unicode znakov.</documentation>
				</annotation>
			</element>
			<element name="BankAccounts" type="bsqr:BankAccounts" bsqr:order="10">
				<annotation>
					<documentation>Zoznam bankových účtov.</documentation>
				</annotation>
			</element>
			<element name="StandingOrderExt" type="bsqr:StandingOrderExt" minOccurs="0"
				bsqr:order="11">
				<annotation>
					<documentation>Rozšírenie platobných údajov o údaje pre nastavenie trvalého príkazu.</documentation>
				</annotation>
			</element>
			<element name="DirectDebitExt" type="bsqr:DirectDebitExt" minOccurs="0" bsqr:order="12">
				<annotation>
					<documentation>Rozšírenie platobných údajov o údaje pre nastavenie a identifikáciu inkasa.</documentation>
				</annotation>
			</element>
        	<element name="BeneficiaryName" type="string" minOccurs="0" bsqr:maxLength="140" bsqr:priority="999" bsqr:order="13">
				<annotation>
					<documentation>Rozšírenie o meno príjemcu</documentation>
				</annotation>
			</element>
        	<element name="BeneficiaryAddressLine1" type="string" minOccurs="0" bsqr:maxLength="70" bsqr:priority="999" bsqr:order="14">
				<annotation>
					<documentation>Rozšírenie o adresu príjemcu</documentation>
				</annotation>
			</element>
        	<element name="BeneficiaryAddressLine2" type="string" minOccurs="0" bsqr:maxLength="70" bsqr:priority="999" bsqr:order="15">
				<annotation>
					<documentation>Rozšírenie o adresu príjemcu (druhý riadok)</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<simpleType name="PaymentOptions">
        <annotation>
        	<documentation>Možnosti platby sa dajú kombinovať. Oddeľujú sa medzerou a treba uviesť vždy aspoň jednu z možností.
paymentorder - platobný príkaz
standingorder - trvalý príkaz, údaje sa vyplnia do StandingOrderExt
directdebit - inkaso, údaje sa vyplnia do DirectDebitExt</documentation></annotation>
        <list itemType="bsqr:PaymentOption" />
	</simpleType>
	<simpleType name="PaymentOption">
        <annotation>
        	<documentation>Možnosť platby.</documentation>
        </annotation>
        <restriction base="string">
			<enumeration value="paymentorder">
				<annotation>
					<appinfo>1</appinfo>
				</annotation>
			</enumeration>
			<enumeration value="standingorder">
				<annotation>
					<appinfo>2</appinfo>
				</annotation>
			</enumeration>
			<enumeration value="directdebit">
				<annotation>
					<appinfo>4</appinfo>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>
	<simpleType name="VariableSymbol">
        <annotation>
        	<documentation>Variabilný symbol je maximálne 10 miestne číslo.</documentation></annotation>
        <restriction base="string">
			<pattern value="[0-9]{0,10}"></pattern>
		</restriction>
	</simpleType>
	<simpleType name="ConstantSymbol">
        <annotation>
        	<documentation>Konštantný symbol je 4 miestne identifikačné číslo.</documentation></annotation>
        <restriction base="string">
			<pattern value="[0-9]{0,4}"></pattern>
		</restriction>
	</simpleType>
	<simpleType name="SpecificSymbol">
        <annotation>
        	<documentation>Špecifický symbol je maximálne 10 miestne číslo.</documentation></annotation>
        <restriction base="string">
			<pattern value="[0-9]{0,10}"></pattern>
		</restriction>
	</simpleType>
	<complexType name="BankAccounts">
        <annotation>
        	<documentation>Zoznam bankových účtov.</documentation></annotation>
        <sequence>
			<annotation>
				<documentation>list of bank accounts</documentation>
			</annotation>
			<element name="BankAccount" type="bsqr:BankAccount" maxOccurs="unbounded" bsqr:order="1">
				<annotation>
					<documentation>Údaje bankového účtu prijímateľa platby.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="BankAccount">
		<annotation>
			<documentation>Bankvý účet v medzinárodnom formáte.</documentation>
		</annotation>
		<sequence>
			<element name="IBAN" type="bsqr:IBAN" bsqr:maxLength="34" bsqr:priority="999"
				bsqr:order="1">
				<annotation>
					<documentation>Medzinárodné číslo bankového účtu vo formáte IBAN. Príklad: "SK8209000000000011424060".

Viac na http://www.sbaonline.sk/sk/projekty/financne-vzdelavanie/slovnik-bankovych-pojmov/iii/.</documentation>
				</annotation>
			</element>
			<element name="BIC" type="bsqr:BIC" bsqr:maxLength="11" bsqr:priority="999"
				bsqr:order="2" minOccurs="0">
				<annotation>
					<documentation>Medzinárodný bankový identifikačný kód (z ang. Bank Identification Code).

Viac na http://www.sbaonline.sk/sk/projekty/financne-vzdelavanie/slovnik-bankovych-pojmov/bbb/bic.html.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<simpleType name="IBAN">
		<annotation>
			<documentation>Medzinárodné číslo bankového účtu vo formáte IBAN. Príklad: &quot;SK8209000000000011424060&quot;.

Viac na http://www.sbaonline.sk/sk/projekty/financne-vzdelavanie/slovnik-bankovych-pojmov/iii/.</documentation>
		</annotation>
		<restriction base="string">
			<pattern value="[A-Z]{2}[0-9]{2}[A-Z0-9]{0,30}" />
		</restriction>
	</simpleType>
	<simpleType name="BIC">
		<annotation>
			<documentation>Medzinárodný bankový identifikačný kód (z ang. Bank Identification Code).

Viac na http://www.sbaonline.sk/sk/projekty/financne-vzdelavanie/slovnik-bankovych-pojmov/bbb/bic.html.</documentation>
		</annotation>
		<restriction base="string">
			<pattern value="[A-Z]{4}[A-Z]{2}[A-Z\d]{2}([A-Z\d]{3})?" />
		</restriction>
	</simpleType>
	<complexType name="StandingOrderExt">
		<annotation>
			<documentation>Rozšírenie platobných údajov o údaje pre nastavenie trvalého príkazu.</documentation>
		</annotation>
		<sequence>
			<element name="Day" type="integer" minOccurs="0" bsqr:maxLength="2"
				bsqr:priority="999" bsqr:order="1">
				<annotation>
					<documentation>Deň platby vyplývajúci z opakovania (Periodicity).
Deň v mesiaci je číslo medzi 1 a 31.
Deň v týždni je číslo medzi 1 a 7 (1 = pondelok, 2 =utorok, …, 7 = nedeľa).</documentation>
				</annotation>
			</element>
			<element name="Month" type="bsqr:MultipleMonths" minOccurs="0" bsqr:maxLength="4"
				bsqr:priority="999" bsqr:order="2">
				<annotation>
					<documentation>Medzerou oddelený zoznam mesiacov, v ktoré sa má platba uskutočniť.</documentation>
				</annotation>
			</element>
			<element name="Periodicity" type="bsqr:Periodicity" bsqr:maxLength="1"
				bsqr:priority="999" bsqr:order="3">
				<annotation>
					<documentation>Opakovanie (periodicita) trvalého príkazu.</documentation>
				</annotation>
			</element>
			<element name="LastDate" type="date" minOccurs="0" bsqr:maxLength="8"
				bsqr:priority="999" bsqr:order="4">
				<annotation>
					<documentation>Dátum poslednej platby v trvalom príkaze.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="DirectDebitExt">
		<annotation>
			<documentation>Rozšírenie platobných údajov o údaje pre nastavenie a identifikáciu inkasa.</documentation>
		</annotation>
		<sequence>
			<element name="DirectDebitScheme" type="bsqr:DirectDebitScheme" bsqr:maxLength="1"
				bsqr:priority="999" bsqr:order="1">
				<annotation>
					<documentation>Inksaná schéma. Uvádza ja jedna z možností:
SEPA - Inkaso zodpovedá schéme SEPA.
other - iné</documentation>
				</annotation>
			</element>
			<element name="DirectDebitType" type="bsqr:DirectDebitType" bsqr:maxLength="1"
				bsqr:priority="999" bsqr:order="2">
				<annotation>
					<documentation>Typ inkasa. Uvádza ja jedna z možností:
one-off - jednorázové inkaso
recurrent - opakované inkaso</documentation>
				</annotation>
			</element>
			<choice minOccurs="0">
				<sequence>
					<element name="VariableSymbol" type="bsqr:VariableSymbol" minOccurs="0"
						bsqr:maxLength="10" bsqr:priority="4" bsqr:order="3">
						<annotation>
							<documentation>Variabilný symbol. Vypĺňa sa len v prípade, ak sa odlišuje od variabilného symbolu v platobnom príkaze.</documentation>
						</annotation>
					</element>
					<element name="SpecificSymbol" type="bsqr:SpecificSymbol" minOccurs="0"
						bsqr:maxLength="10" bsqr:priority="3" bsqr:order="4">
						<annotation>
							<documentation>Špecifický symbol. Vypĺňa sa len v prípade, ak sa odlišuje od špecifického symbolu v platobnom príkaze.</documentation>
						</annotation>
					</element>
				</sequence>
				<element name="OriginatorsReferenceInformation" type="string" minOccurs="0"
					bsqr:maxLength="35" bsqr:priority="11" bsqr:order="5">
					<annotation>
						<documentation>Referenčná informácia. Použije sa len na prechodné obdobie z variabilného a špecifického symbolu na SEPA inkaso.</documentation>
					</annotation>
				</element>
				<sequence>
					<element name="MandateID" type="string" bsqr:maxLength="35" bsqr:priority="10"
						bsqr:order="6">
						<annotation>
							<documentation>Identifikácia mandátu medzi veriteľom a dlžníkom podľa SEPA.</documentation>
						</annotation>
					</element>
					<element name="CreditorID" type="string" bsqr:maxLength="35" bsqr:priority="9"
						bsqr:order="7">
						<annotation>
							<documentation>Identifikácia veriteľa podľa SEPA.</documentation>
						</annotation>
					</element>
					<element name="ContractID" type="string" minOccurs="0" bsqr:maxLength="35"
						bsqr:priority="8" bsqr:order="8">
						<annotation>
							<documentation>Identifikácia zmluvy medzi veriteľom a dlžníkom podľa SEPA.</documentation>
						</annotation>
					</element>
				</sequence>
			</choice>
			<element name="MaxAmount" type="decimal" minOccurs="0" bsqr:maxLength="15"
				bsqr:priority="999" bsqr:order="9">
				<annotation>
					<documentation>Maximálna čiastka inkasa.</documentation>
				</annotation>
			</element>
			<element name="ValidTillDate" type="date" minOccurs="0" bsqr:maxLength="8"
				bsqr:priority="999" bsqr:order="10">
				<annotation>
					<documentation>Dátum platnosti inkasa. Platnosť inkasa zaníka dňom tohto dátumu.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<simpleType name="DirectDebitScheme">
		<annotation>
			<documentation>Inksaná schéma.
SEPA - Inkaso zodpovedá schéme SEPA.
other - iné</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="other">
				<annotation>
					<appinfo>0</appinfo>
